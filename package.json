  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --ext ts,js,json,xlsx,html -r ts-node/register/transpile-only src/index",
    "start:nod": "nodemon --exec ts-node src/index",
    "start:dev": "cross-env NODE_ENV=development node src/index",
    "start:staging": "cross-env NODE_ENV=staging node src/index",
    "start:staging-bci": "cross-env NODE_ENV=staging-bci node src/index",
    "start:prod": "cross-env NODE_ENV=production node  src/index",
    "build": "tsc -p . && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.json src/**/*.html src/**/*.xlsx dist/src/",
    "scripts": "node -r ts-node/register/transpile-only scripts/index",
    "drop-test": "node -r ts-node/register/transpile-only scripts/drop-test"
  },
